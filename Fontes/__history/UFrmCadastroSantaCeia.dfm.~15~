  object QryLkpMembros: TMyQuery
    Connection = DMPrincipal.Conn
    SQL.Strings = (
      'SELECT CodMembro,CodFamilia,NomeMembro'
      'From Membros'
      'where not isnull(DataConfirmacao)'
      'and isnull(codMotivoSaida)'
      'Order by NomeMembro')
