  object QrySantaCeia: TMyQuery
    SQLInsert.Strings = (
      'INSERT INTO santa_ceia_antigo'
      
        '  (CodMembro, CodFamilia, ano, janeiro, fevereiro, marco, abril,' +
        ' maio, junho, julho, agosto, setembro, outubro, novembro, dezemb' +
        'ro, CodSantaCeia)'
      'VALUES'
      
        '  (:CodMembro, :CodFamilia, :ano, :janeiro, :fevereiro, :marco, ' +
        ':abril, :maio, :junho, :julho, :agosto, :setembro, :outubro, :no' +
        'vembro, :dezembro, :CodSantaCeia)')
    SQLDelete.Strings = (
      'DELETE FROM santa_ceia_antigo'
      'WHERE'
      '  CodSantaCeia = :Old_CodSantaCeia')
    SQLUpdate.Strings = (
      'UPDATE santa_ceia_antigo'
      'SET'
      
        '  CodMembro = :CodMembro, CodFamilia = :CodFamilia, ano = :ano, ' +
        'janeiro = :janeiro, fevereiro = :fevereiro, marco = :marco, abri' +
        'l = :abril, maio = :maio, junho = :junho, julho = :julho, agosto' +
        ' = :agosto, setembro = :setembro, outubro = :outubro, novembro =' +
        ' :novembro, dezembro = :dezembro, CodSantaCeia = :CodSantaCeia'
      'WHERE'
      '  CodSantaCeia = :Old_CodSantaCeia')
    SQLRefresh.Strings = (
      
        'SELECT CodMembro, CodFamilia, ano, janeiro, fevereiro, marco, ab' +
        'ril, maio, junho, julho, agosto, setembro, outubro, novembro, de' +
        'zembro, CodSantaCeia FROM santa_ceia_antigo'
      'WHERE'
      '  CodSantaCeia = :CodSantaCeia')
    SQLLock.Strings = (
      'SELECT * FROM santa_ceia_antigo'
      'WHERE'
      '  CodSantaCeia = :Old_CodSantaCeia'
      'FOR UPDATE')
    Connection = DMPrincipal.Conn
    SQL.Strings = (
      'SELECT sca.*, F.Sobrenome, m.NomeMembro'
      
        'FROM santa_ceia_antigo sca left join Familia F on sca.codfamilia' +
        ' = f.codfamilia'
      
        '                           left join Membros M on sca.CodMembro ' +
        '= M.CodMembro'
      'order by ano desc, codfamilia, codmembroantigo')
    Left = 332
    Top = 4
