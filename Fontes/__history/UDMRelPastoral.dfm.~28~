  object QueryVisitas: TMyQuery
    Connection = DMPrincipal.Conn
    SQL.Strings = (
      'SELECT DataVisita, Visitante'
      'FROM visita v'
      'where codfamilia = :pCodFamilia'
      'order by datavisita desc limit 1;')
