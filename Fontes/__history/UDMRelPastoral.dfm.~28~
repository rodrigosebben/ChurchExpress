  object QuerySantaCeia: TMyQuery
    Connection = DMPrincipal.Conn
    SQL.Strings = (
      
        'SELECT concat(F.sobrenome, '#39'-'#39', s.CodFamilia) as FamiliaCodigo,s' +
        '.CodFamilia, s.codmembroantigo,s.Codmembro, F.Sobrenome as Famil' +
        'ia, m.NomeMembro as Membro,t.nometipomembro,M.DataNasc, M.envelo' +
        'pe, M.Telefone as TelefoneMembro, M.Codmotivosaida, s.ano, F.end' +
        'ereco, f.bairro, f.cidade, f.area, f.grupo, f.uf, f.cep, f.telef' +
        'one, f.email,'
      
        '      (SELECT DataVisita FROM visita v where v.codfamilia = s.co' +
        'dfamilia order by DATAVISITA desc limit 1) DataUltimaVisita,'
      
        '      (SELECT Visitante FROM visita v where v.codfamilia = s.cod' +
        'familia order by DATAVISITA desc limit 1) UltimoVisitante,'
      '       janeiro,'
      '       fevereiro,'
      '       marco,'
      '       abril,'
      '       maio,'
      '       junho,'
      '       julho,'
      '       agosto,'
      '       setembro,'
      '       outubro,'
      '       novembro,'
      '       dezembro'
      
        'FROM santa_ceia_antigo s left join Familia F on s.codfamilia = f' +
        '.codfamilia'
      
        '                         left join Membros M on s.codmembro = m.' +
        'codmembro'
      
        '                         left join TipoMembro t on m.codtipomemb' +
        'ro = t.codtipomembro'
      
        'Order by f.Sobrenome, s.codfamilia,s.codmembroantigo, Membro, an' +
        'o')
    Left = 284
    Top = 12
