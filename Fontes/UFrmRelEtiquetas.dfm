  object QryRelExternas: TMyQuery
    Connection = DMPrincipal.Conn
    SQL.Strings = (
      
        'select m.ChefeFamilia, m.Envelope, m.codMembro, m.codmembroantig' +
        'o, m.codfamilia, m.DataNasc,'
      
        '       concat(m.CodFamilia,'#39'-'#39',m.CodMembroAntigo) as FamiliaMemb' +
        'ro,'
      '       DATE_FORMAT(m.DataNasc,'#39'%d/%m'#39') as Anivers'#225'rio,'
      '       m.NomeMembro,'
      '       concat_ws('#39','#39',m.Endereco,m.Compl) as EnderecoCompleto,'
      '       m.Bairro,'
      '       m.Cep,'
      '       m.Cidade,'
      '       m.UF,'
      
        '       DATE_FORMAT(NOW( ),'#39'%Y'#39') - DATE_FORMAT(m.datanasc,'#39'%Y'#39') -' +
        ' (DATE_FORMAT(NOW( ),'#39'00-%m-%d'#39') < DATE_FORMAT(m.datanasc,'#39'00-%m' +
        '-%d'#39')) AS Idade'
      
        'from Membros m left join familia F on m.codfamilia = f.codfamili' +
        'a'
      'where isnull(m.CodMotivoSaida)'
      'Order by f.sobrenome, m.codmembroantigo')
    Left = 460
    Top = 268
