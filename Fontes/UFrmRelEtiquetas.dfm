  object QryRelInternas: TMyQuery
    SQLInsert.Strings = (
      'INSERT INTO etiquetastemp'
      
        '  (CodEtiqueta, CodMembro, CodMembroAntigo, CodFamilia, NomeMemb' +
        'ro, DataNasc, MesMensagem, AnoMensagem, Mensagem, Envelope, Mes)'
      'VALUES'
      
        '  (:CodEtiqueta, :CodMembro, :CodMembroAntigo, :CodFamilia, :Nom' +
        'eMembro, :DataNasc, :MesMensagem, :AnoMensagem, :Mensagem, :Enve' +
        'lope, :Mes)')
    SQLDelete.Strings = (
      'DELETE FROM etiquetastemp'
      'WHERE'
      '  CodEtiqueta = :Old_CodEtiqueta')
    SQLUpdate.Strings = (
      'UPDATE etiquetastemp'
      'SET'
      
        '  CodEtiqueta = :CodEtiqueta, CodMembro = :CodMembro, CodMembroA' +
        'ntigo = :CodMembroAntigo, CodFamilia = :CodFamilia, NomeMembro =' +
        ' :NomeMembro, DataNasc = :DataNasc, MesMensagem = :MesMensagem, ' +
        'AnoMensagem = :AnoMensagem, Mensagem = :Mensagem, Envelope = :En' +
        'velope, Mes = :Mes'
      'WHERE'
      '  CodEtiqueta = :Old_CodEtiqueta')
    SQLRefresh.Strings = (
      
        'SELECT CodEtiqueta, CodMembro, CodMembroAntigo, CodFamilia, Nome' +
        'Membro, DataNasc, MesMensagem, AnoMensagem, Mensagem, Envelope, ' +
        'Mes FROM etiquetastemp'
      'WHERE'
